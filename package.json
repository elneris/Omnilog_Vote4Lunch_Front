{
  "name": "vote4lunch",
  "version": "0.8.0",
  "private": true,
  "description": "Vote 4 Lunch app",
  "author": "Fabien Schlegel",
  "main": "./server/server.js",
  "proxy": "http://localhost:5000",
  "engines": {
    "node": "10.11.0"
  },
  "jest": {
    "setupFiles": [
      "chai/register-should"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/test-setup.js",
    "testEnvironment": "<rootDir>/server/should-environment.js",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "server/**/*.{js,jsx}"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|sass|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "scripts": {
    "lint": "eslint src && eslint server",
    "build": "react-scripts build",
    "start": "node ./server/bin/start",
    "dev": "concurrently \"npm start\" \" react-scripts start\"",
    "test": "jest",
    "eject": "react-scripts eject",
    "import-data": "node ./node_modules/@babel/node/bin/babel-node.js commands/importData.js",
    "sync": "node ./node_modules/@babel/node/bin/babel-node.js server/sequelize.js",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.4.0",
    "@fortawesome/free-regular-svg-icons": "^5.4.0",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "axios": "^0.18.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "cookie-parser": "^1.4.3",
    "debug": "^3.0.0",
    "dotenv": "^6.1.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "express": "^4.16.3",
    "is-touch-device": "^1.0.1",
    "jest": "^23.6.0",
    "jest-environment-jsdom": "^23.4.0",
    "jsonwebtoken": "^8.5.1",
    "leaflet": "^1.3.4",
    "material-icons-react": "^1.0.4",
    "moment": "^2.22.2",
    "morgan": "^1.8.2",
    "node-sass": "^4.10.0",
    "npm-run-all": "^4.1.3",
    "pg": "^7.5.0",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.5.0",
    "react-leaflet": "^2.0.1",
    "react-leaflet-control": "^2.0.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.3",
    "react-test-renderer": "^16.6.0",
    "reactstrap": "^6.4.0",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-thunk": "^2.3.0",
    "sequelize": "^4.39.0",
    "shortid": "^2.2.13",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "concurrently": "^4.0.1",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "moxios": "^0.4.0",
    "react-addons-test-utils": "^15.6.2",
    "redux-mock-store": "^1.5.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
