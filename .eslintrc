{
  "parser": "babel-eslint",
  "extends": ["airbnb"
  ],
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "ecmaFeatures": {
    "classes": true,
    "modules": true
  },
  "rules": {
    "jsx-a11y/href-no-hash": "off",
    "comma-dangle": 0,
    "linebreak-style": 0,
    "react/jsx-uses-react": 2,
    "react/require-default-props": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  },
  "plugins": [
    "react"
  ]
}